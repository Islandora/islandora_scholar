<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="../style/module.stylesheet.xsl" ?>
<exslt:module xmlns:exslt="http://exslt.org/documentation"
              version="1" prefix="regexp">

<exslt:name>Regular Expressions</exslt:name>

<rdf:Description xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
                 xmlns:dc="http://purl.org/dc/elements/1.1/"
                 ID="regexp">
   <dc:subject>EXSLT</dc:subject>
   <dc:subject>regexp</dc:subject>
   <dc:subject>module</dc:subject>
   <dc:subject>regular expression</dc:subject>
   <exslt:revision>
      <rdf:Description xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
                       xmlns:dc="http://purl.org/dc/elements/1.1/"
                       ID="regexp.1">
         <exslt:version>1</exslt:version>
         <dc:creator>Jeni Tennison</dc:creator>
         <dc:date>2001-06-30</dc:date>
         <dc:description>A module of regular expression functions.</dc:description>
      </rdf:Description>
   </exslt:revision>
</rdf:Description>

<exslt:doc xmlns="">
   <section>
      <para>
         EXSLT - Regular Expressions covers extension elements and functions that provide facilities to do with regular expressions.
      </para>
      <para>
         For ease of implementation, the regular expressions used in this module currently use the Javascript regular expression syntax.
      </para>
   </section>
</exslt:doc>

<exslt:functions>
   <exslt:function name="test" version="1" core="no" />
   <exslt:function name="match" version="1" core="no" />
   <exslt:function name="replace" version="1" core="no" />
</exslt:functions>

</exslt:module>